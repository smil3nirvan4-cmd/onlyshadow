{
  "version": 2,
  "builds": [
    {
      "src": "api/main.py",
      "use": "@vercel/python"
    },
    {
      "src": "dashboard/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(?<path>.*)",
      "dest": "/api/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "/dashboard/index.html"
    }
  ]
}
